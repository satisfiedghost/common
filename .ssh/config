# Argus Cameras
Host argus-em em-argus
  Hostname argus.sf
  User root
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking no

Host flight-model-argus fm-argus argus-fm
  Hostname 172.18.0.2
  User root
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking no
	ProxyJump flight-model-nx
  
Host argus-dev-kit argus-dev
  Hostname argus-dev.sf
  User root
  IdentityFile ~/.ssh/id_rsa
  StrictHostKeyChecking no

# Misc. Office Computers
Host rpi
  Hostname 192.168.2.244
  User ubuntu
  IdentityFile ~/.ssh/id_rsa
  ForwardX11 yes

# Flatsat
Host flatsat-fc em-fc emfc
	Hostname 10.100.10.115
	User root
	IdentityFile ~/.ssh/id_rsa

Host flatsat-nx em-nx emnx
	Hostname 172.17.0.10
	User nx
	IdentityFile ~/.ssh/id_rsa
	ProxyJump flatsat-fc

# Flight Model
Host flight-model-fc fm-fc fmfc
	Hostname 10.100.10.128
	User root
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host flight-model-nx fm-nx fmnx
	Hostname 172.17.0.10
	User nx
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking no
	ProxyJump flight-model-fc

